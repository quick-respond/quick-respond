{
  "name": "@quick-respond/web",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --disable-host-check --host 0.0.0.0 --port 80",
    "serve": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --disable-host-check --host 0.0.0.0 --port 4200",
    "build": "bash bash/angular-build.sh",
    "test": "ng test",
    "docker": "npm run docker:remove && docker compose up -d app-web --build --force-recreate",
    "docker:inspect": "docker exec -it quick-respond-app-web bash",
    "docker:remove": "docker rm -f quick-respond-app-web",
    "docker:build": "bash bash/docker-build.sh",
    "docker:push": "bash bash/docker-push.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.6",
    "@angular/common": "^19.2.6",
    "@angular/compiler": "^19.2.6",
    "@angular/core": "^19.2.6",
    "@angular/forms": "^19.2.6",
    "@angular/platform-browser": "^19.2.6",
    "@angular/platform-browser-dynamic": "^19.2.6",
    "@angular/router": "^19.2.6",
    "crypto-js": "^4.2.0",
    "flowbite": "^3.1.2",
    "fs": "^0.0.1-security",
    "jsqr": "^1.4.0",
    "luxon": "^3.4.4",
    "mathjs": "^14.4.0",
    "ngx-cookie-service": "^19.1.0",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.7",
    "@angular/cli": "~19.2.7",
    "@angular/compiler-cli": "^19.2.6",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/crypto-js": "^4.2.2",
    "@types/jasmine": "~5.1.4",
    "@types/luxon": "^3.4.2",
    "@types/qrcode": "^1.5.5",
    "autoprefixer": "^10.4.20",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.28",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.8.3"
  }
}
